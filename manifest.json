{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Overwolf Debug Issue",
    "author": "Leon Machens",
    "version": "0.0.1",
    "minimum-overwolf-version": "0.169.0",
    "description": "Overwolf Debug Issue",
    "dock_button_title": "Overwolf Debug Issue",
    "icon": "icons/iconMouseOver.png",
    "icon_gray": "icons/iconMouseNormal.png",
    "launcher_icon": "icons/desktopIcon.ico",
    "window_icon": "icons/taskbarIcon.png"
  },
  "permissions": [
    "Extensions",
    "FileSystem",
    "Streaming",
    "DesktopStreaming",
    "VideoCaptureSettings"
  ],
  "data": {
    "start_window": "desktop",
    "windows": {
      "desktop": {
        "file": "index.html",
        "debug_url": "http://localhost:3333/index.html",
        "size": {
          "width": 1212,
          "height": 699
        },
        "allow_local_file_access": true
      }
    },
    "game_targeting": {
      "type": "dedicated",
      "game_ids": []
    },
    "game_events": [],
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": []
        },
        "start_minimized": false
      }
    ],
    "hotkeys": {},
    "protocol_override_domains": {
      "googlesyndication": "http"
    },
    "externally_connectable": {
      "matches": []
    },
    "force_browser": "user",
    "developer": {
      "enable_auto_refresh": true,
      "reload_delay": 1000,
      "filter": "*.*"
    }
  }
}
